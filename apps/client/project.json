{
  "name": "client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/client/src",
  "prefix": "app",
  "generators": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "less",
      "changeDetection": "OnPush",
      "skipTests": true
    }
  },
  "targets": {
    "build": {
      "executor": "@nx/angular:browser-esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/client",
        "index": "apps/client/src/index.html",
        "main": "apps/client/src/main.ts",
        "polyfills": "apps/client/src/polyfills.ts",
        "tsConfig": "apps/client/tsconfig.app.json",
        "assets": [
          "apps/client/src/favicon.ico",
          "apps/client/src/assets",
          {
            "glob": "**/*",
            "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
            "output": "/assets/"
          },
          {
            "glob": "!(db)/*.json",
            "input": "./libs/data/src/lib/json/",
            "output": "/assets/data/"
          },
          {
            "glob": "*.json",
            "input": "./libs/data/src/lib/json/",
            "output": "/assets/data/"
          },
          {
            "glob": "**/*",
            "input": "./libs/data/src/lib/extracts/",
            "output": "/assets/extracts/"
          },
          "apps/client/src/oembed.json"
        ],
        "styles": ["apps/client/src/cdk.less", "apps/client/src/styles.less", "apps/client/src/theme.less"],
        "scripts": ["node_modules/marked/marked.min.js"]
      },
      "configurations": {
        "dev": {
          "optimization": false,
          "sourceMap": true,
          "namedChunks": true,
          "aot": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": false,
          "baseHref": "/",
          "deployUrl": "/"
        },
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "aot": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "baseHref": "/",
          "deployUrl": "https://cdn.ffxivteamcraft.com/",
          "fileReplacements": [
            {
              "replace": "apps/client/src/environments/environment.ts",
              "with": "apps/client/src/environments/environment.prod.ts"
            }
          ]
        },
        "functions": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "aot": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "baseHref": "/",
          "fileReplacements": [
            {
              "replace": "apps/client/src/environments/environment.ts",
              "with": "apps/client/src/environments/environment.prod.ts"
            }
          ]
        },
        "electron": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": true,
          "namedChunks": false,
          "aot": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "baseHref": "",
          "fileReplacements": [
            {
              "replace": "apps/client/src/environments/environment.ts",
              "with": "apps/client/src/environments/environment.prod.ts"
            }
          ],
          "assets": [
            "apps/client/src/favicon.ico",
            "apps/client/src/assets",
            {
              "glob": "**/*",
              "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
              "output": "/assets/"
            },
            {
              "glob": "!(db)/*.json",
              "input": "./libs/data/src/lib/json/",
              "output": "/assets/data/"
            },
            {
              "glob": "*",
              "input": "./libs/data/src/lib/json/",
              "output": "/assets/data/"
            },
            {
              "glob": "**/*",
              "input": "./libs/data/src/lib/extracts/",
              "output": "/assets/extracts/"
            }
          ]
        },
        "electron-dev": {
          "optimization": false,
          "sourceMap": true,
          "namedChunks": true,
          "aot": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": false,
          "baseHref": "",
          "assets": [
            "apps/client/src/favicon.ico",
            "apps/client/src/assets",
            {
              "glob": "**/*",
              "input": "./node_modules/@ant-design/icons-angular/src/inline-svg/",
              "output": "/assets/"
            },
            {
              "glob": "!(db)/*.json",
              "input": "./libs/data/src/lib/json/",
              "output": "/assets/data/"
            },
            {
              "glob": "*",
              "input": "./libs/data/src/lib/json/",
              "output": "/assets/data/"
            },
            {
              "glob": "**/*",
              "input": "./libs/data/src/lib/extracts/",
              "output": "/assets/extracts/"
            }
          ]
        },
        "beta": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": true,
          "namedChunks": false,
          "aot": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "baseHref": "/",
          "deployUrl": "/",
          "fileReplacements": [
            {
              "replace": "apps/client/src/environments/environment.ts",
              "with": "apps/client/src/environments/environment.beta.ts"
            }
          ]
        },
        "preview": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "aot": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "baseHref": "/",
          "fileReplacements": [
            {
              "replace": "apps/client/src/environments/environment.ts",
              "with": "apps/client/src/environments/environment.preview.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "dev"
    },
    "serve": {
      "executor": "@nx/angular:dev-server",
      "options": {
        "buildTarget": "client:build"
      },
      "configurations": {
        "dev": {
          "buildTarget": "client:build:dev"
        },
        "production": {
          "buildTarget": "client:build:production"
        },
        "beta": {
          "buildTarget": "client:build:beta"
        },
        "preview": {
          "buildTarget": "client:build:preview"
        }
      },
      "defaultConfiguration": "dev"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/client/**/*.ts", "apps/client/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/apps/client"],
      "options": {
        "jestConfig": "apps/client/jest.config.ts"
      }
    }
  },
  "tags": []
}
