{
  "name": "ffxiv-teamcraft-beta",
  "description": "Collaborative crafting made easy",
  "homepage": "https://ffxivteamcraft.com",
  "author": "Flavien Normand <contact@flavien-normand.fr>",
  "version": "6.0.0",
  "license": "MIT",
  "main": "desktop/main.js",
  "build": {
    "appId": "ffxivteamcraft",
    "forceCodeSigning": true,
    "directories": {
      "output": "release"
    },
    "files": [
      "desktop/main.js",
      "desktop/oauth.js",
      "desktop/machina.js",
      "dist"
    ],
    "extraResources": [
      {
        "from": "./node_modules/node-machina-ffxiv/MachinaWrapper",
        "to": "MachinaWrapper",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./desktop",
        "to": "MachinaWrapper",
        "filter": [
          "**/*.dll"
        ]
      }
    ],
    "protocols": {
      "name": "ffxiv-teamcraft",
      "schemes": [
        "teamcraft"
      ]
    },
    "mac": {
      "category": "public.app-category.games"
    },
    "win": {
      "icon": "./tools/build/icon.ico",
      "publisherName": "FFXIV Teamcraft",
      "requestedExecutionLevel": "highestAvailable",
      "publish": {
        "provider": "github",
        "releaseType": "release"
      }
    },
    "nsis": {
      "include": "./tools/build/nsis/include.nsh",
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  },
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "ng": "ng",
    "start": "node ./tools/build/prebuild.js && ng serve --project=client",
    "build:watch": "node ./tools/build/prebuild.js && npm run ng-high-memory build -- --project=client --watch --configuration=electron-dev",
    "build:prod": "node ./tools/build/prebuild.js && npm run ng-high-memory build -- --project=client --prod --no-progress",
    "build:functions": "node ./tools/build/prebuild.js && npm run ng-high-memory build -- --project=client --configuration=functions --no-progress",
    "build:electron-prod": "node ./tools/build/prebuild.js && npm run ng-high-memory build -- --project=client --configuration=electron",
    "build:electron-dev": "node ./tools/build/prebuild.js && npm run ng-high-memory build -- --project=client --configuration=electron-dev",
    "build:beta": "node ./tools/build/prebuild.js && npm run ng-high-memory build -- --project=client --configuration=beta --no-progress",
    "standard-version": "standard-version",
    "standard-version:patch": "standard-version --release-as patch",
    "standard-version:minor": "standard-version --release-as minor",
    "standard-version:dry": "standard-version --dry-run",
    "release:prod": "git push --follow-tags origin master",
    "release:beta": "npm run standard-version -- --prerelease beta && git push --follow-tags origin beta",
    "test": "node ./tools/build/prebuild.js && ng test --project=client --watch=false --code-coverage",
    "codecov": "cat coverage/lcov.info | codecov",
    "lint": "ng lint && jsonlint-cli ./src/assets/i18n/*.json",
    "e2e": "node ./tools/build/prebuild.js && ng e2e client-e2e",
    "e2e:watch": "node ./tools/build/prebuild.js && ng e2e client-e2e --watch",
    "e2e:ci": "node ./tools/build/prebuild.js && ng e2e client-e2e --headless",
    "commitmsg": "validate-commit-msg",
    "prerelease": "npm test",
    "commit": "git-cz",
    "electron:start": "electron ./desktop/main.js",
    "electron:start:no-decorators": "electron ./desktop/main.js --native-titlebar",
    "electron:setup:build": "del-cli release/* && npm run build:electron-prod && electron-builder --win",
    "electron:setup:build-dev": "del-cli release/* && npm run build:electron-dev && electron-builder --win --dir",
    "electron:setup:build-beta": "del-cli release/* && npm run build:electron-dev && electron-builder --win",
    "electron:setup:publish": "del-cli release/* && npm run build:electron-prod && electron-builder --win -p always",
    "postinstall": "electron-builder install-app-deps",
    "release:full": "npm run build:prod && firebase deploy -P default --only hosting && npm run electron:setup:publish",
    "pack": "electron-builder --dir",
    "update": "ng update @nrwl/schematics",
    "update:check": "ng update",
    "electron-builder": "electron-builder",
    "***********SSR STUFF***************": "",
    "build:ssr": "npm run gt:fetch && npm run webpack:ssr & node ssr/copy-translations.js & npm run copy:ssr & exit 0",
    "build:ssr:ci": "npm run build:functions && npm run build:functions && npm run gt:fetch && npm run webpack:ssr; node ssr/copy-translations.js; cp -r ./dist functions; exit 0",
    "webpack:ssr": "npm run ng-high-memory run client:server && webpack --config webpack.server.config.js --no-progress",
    "serve:ssr": "node dist/client-webpack/server.js",
    "start:ssr": "npm run build:ssr & npm run serve:ssr",
    "gt:fetch": "node ssr/gt-data-fetcher.js",
    "copy:ssr": "copyfiles ./dist/** ./functions",
    "*********** STATS STUFF **************": "",
    "build:stats": "node ./tools/build/prebuild.js && npm run ng-high-memory build -- --project=client --prod --stats-json",
    "bundle-analyzer": "webpack-bundle-analyzer dist/apps/client/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "7.2.7",
    "@angular/compiler": "7.2.7",
    "@angular/core": "7.2.7",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "7.2.7",
    "@angular/http": "7.2.7",
    "@angular/platform-browser": "7.2.7",
    "@angular/platform-browser-dynamic": "7.2.7",
    "@angular/platform-server": "7.2.7",
    "@angular/router": "7.2.7",
    "@ffxiv-teamcraft/crafting-solver": "^1.2.0",
    "@ffxiv-teamcraft/simulator": "^1.3.3",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@kaiu/ng-serializer": "1.1.1",
    "@kaiu/serializer": "1.4.0",
    "@ng-toolkit/universal": "^7.1.2",
    "@ngrx/effects": "7.4.0",
    "@ngrx/entity": "7.4.0",
    "@ngrx/router-store": "7.4.0",
    "@ngrx/store": "7.4.0",
    "@nguniversal/common": "^7.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@nrwl/nx": "7.5.1",
    "@swimlane/dragula": "^3.7.3",
    "@swimlane/ngx-charts": "^12.0.1",
    "@swimlane/ngx-dnd": "^7.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.123",
    "@types/semver": "^5.4.0",
    "@xivapi/angular-client": "^1.4.1",
    "angular-feather": "^5.3.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "deep-diff": "^1.0.2",
    "electron-config": "^2.0.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^3.0.6",
    "electron-updater": "^4.0.6",
    "express": "^4.16.4",
    "file-saver": "^2.0.1",
    "firebase": "6.1.0",
    "hammerjs": "^2.0.8",
    "is-elevated": "^3.0.0",
    "jsdom": "^15.0.0",
    "jwt-decode": "^2.2.0",
    "less": "^2.7.3",
    "lodash": "^4.17.10",
    "ng-drag-drop": "^5.0.0",
    "ng-push": "^0.2.1",
    "ng-zorro-antd": "7.4.1",
    "ngx-clipboard": "11.0.0",
    "ngx-color-picker": "^7.5.0",
    "ngx-markdown": "^1.6.0",
    "node-machina-ffxiv": "^2.11.0",
    "papaparse": "^4.6.3",
    "rxjs": "^6.5.1",
    "semver": "^5.4.1",
    "tslib": "^1.9.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.4.3",
    "@angular-devkit/build-angular": "~0.13.8",
    "@angular-devkit/schematics": "^0.8.8",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "7.2.7",
    "@angular/language-service": "7.2.7",
    "@firebase/auth-types": "^0.7.0",
    "@google-cloud/firestore": "^2.3.0",
    "@ngrx/schematics": "^7.0.0",
    "@ngrx/store-devtools": "7.0.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@nrwl/builders": "7.5.1",
    "@nrwl/schematics": "7.5.1",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "cli-progress": "^2.1.1",
    "codecov": "^3.1.0",
    "codelyzer": "~4.2.1",
    "colors": "^1.3.2",
    "commitizen": "^2.9.6",
    "copyfiles": "^2.1.0",
    "cypress": "3.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "del-cli": "^1.1.0",
    "electron": "5.0.3",
    "electron-builder": "20.43.0",
    "firebase-tools": "^4.0.1",
    "firestore-export-import": "^0.2.9",
    "fs-extra": "^7.0.1",
    "fuzzy": "^0.1.3",
    "husky": "^0.14.3",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine": "2.99.0",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "0.3.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jsonlint-cli": "^1.0.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "multi-progress": "^2.0.0",
    "ngrx-store-freeze": "0.2.4",
    "node-sass": "^4.9.0",
    "prettier": "1.13.7",
    "protractor": "~5.3.0",
    "rxjs-compat": "^6.1.0",
    "standard-version": "^4.4.0",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.2.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6",
    "validate-commit-msg": "^2.14.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.2",
    "ws": "^6.0.0",
    "xhr2": "^0.1.4"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}
