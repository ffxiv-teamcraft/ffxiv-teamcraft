    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less": "^3.11.1",
    "memoizee": "^0.4.14",
    "multi-progress": "^2.0.0",
    "ng-drag-drop": "^5.0.0",
    "ng-push": "^0.2.2",
    "ng-zorro-antd": "^10.1.2",
    "ngx-color-picker": "^10.1.0",
    "ngx-markdown": "^10.1.1",
    "nz-tslint-rules": "^0.901.2",
    "papaparse": "^5.2.0",
    "prettier": "^2.0.5",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.6.3",
    "semver": "^5.4.1",
    "standard-version": "^4.4.0",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^8.0.11",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.5",
    "validate-commit-msg": "^2.14.0",
    "web-animations-js": "^2.3.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.3.2",
    "ws": "^6.0.0",
    "xhr2": "^0.1.4",
    "zone.js": "~0.10.2"
  },
  "resolutions": {
    "webpack": "4.44.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  }
}
