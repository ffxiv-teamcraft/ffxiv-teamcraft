    "lodash": "^4.17.21",
    "native-is-elevated": "^0.4.3",
    "ngx-google-analytics": "^11.2.1",
    "request": "^2.88.2",
    "rxjs": "~7.5.0",
    "sqlite3": "^5.1.4",
    "tslib": "^2.3.1",
    "universal-analytics": "^0.5.3",
    "uuid": "^8.3.2"
  },
