{
  "name": "ffxiv-teamcraft",
  "description": "Collaborative crafting made easy",
  "homepage": "https://ffxivteamcraft.com",
  "author": "FFXIV Teamcraft",
  "version": "11.4.13",
  "license": "MIT",
  "main": "dist/apps/electron/main.js",
  "build": {
    "appId": "ffxiv-teamcraft",
    "productName": "FFXIV Teamcraft",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist"
    ],
    "extraFiles": [
      {
        "from": "./node_modules/@ffxiv-teamcraft/pcap-ffxiv/lib/deucalion",
        "to": "deucalion",
        "filter": [
          "**/*.dll"
        ]
      }
    ],
    "asarUnpack": "./dist/apps/electron/workers/lodestone.js",
    "protocols": {
      "name": "ffxiv-teamcraft",
      "schemes": [
        "teamcraft"
      ]
    },
    "mac": {
      "category": "public.app-category.games"
    },
    "win": {
      "icon": "./tools/build/icon.ico",
      "publisherName": "FFXIV Teamcraft",
      "target": "squirrel",
      "sign": "./tools/build/sign-windows.js",
      "publish": {
        "provider": "github",
        "releaseType": "release"
      }
    }
  },
  "scripts": {
    "prebuild": "node ./tools/build/prebuild.js && yarn update-lazy-files && yarn web:copyfiles",
    "release:print": "node ./tools/build/release-notes-print.js",
    "assets:hash": "node ./tools/build/prod-assets-hash.js",
    "start": "yarn nx serve --project=client",
    "build:watch": "yarn prebuild && yarn nx build --project=client --watch --configuration=electron-dev",
    "build:prod": "yarn prebuild && nx run client:build:production && yarn assets:hash",
    "build:functions": "yarn prebuild && yarn nx build --project=client --configuration=functions --no-progress",
    "build:electron-prod": "yarn prebuild && yarn nx build --project=client --configuration=electron && yarn assets:hash",
    "build:electron-dev": "yarn prebuild && yarn nx build --project=client --configuration=electron-dev",
    "build:beta": "yarn prebuild && yarn nx build --project=client --configuration=beta --no-progress",
    "standard-version": "standard-version",
    "standard-version:patch": "git fetch origin && standard-version --release-as patch && yarn standard-version:after",
    "standard-version:minor": "git fetch origin && standard-version --release-as minor && yarn standard-version:after",
    "standard-version:major": "git fetch origin && standard-version --release-as major && yarn standard-version:after",
    "standard-version:dry": "standard-version --dry-run",
    "standard-version:after": "yarn prebuild && git commit -am \"chore(release): Patch notes update\" && yarn release:print",
    "release:prod": "git push --follow-tags origin master",
    "release:beta": "yarn standard-version --prerelease beta && git push --follow-tags origin beta",
    "test": "jest",
    "lint": "nx lint",
    "commitmsg": "validate-commit-msg",
    "prerelease": "npm test",
    "commit": "git-cz",
    "web:copyfiles": "copyfiles ./libs/loader/tc-loader.js ./apps/client/src/assets/ -f",
    "update-lazy-files": "node ./tools/build/lazy-files-updater.js",
    "data-extractor": "nx run data-extraction:build && node --max-old-space-size=8192 dist/apps/data-extraction/main.js",
    "data-extractor:db": "yarn data-extractor --only-db && yarn update-lazy-files",
    "profits-api:build": "tsc -p apps/profits-api/tsconfig.json",
    "************ELECTRON STUFF*****************": "",
    "electron": "electron",
    "electron:copyfiles": "copyfiles ./libs/loader/tc-loader.js ./dist/apps/electron/ -f",
    "electron:build": "nx run electron:build && yarn electron:copyfiles && tsc -p ./apps/electron/tsconfig.preload.json",
    "electron:start": "yarn electron:build && yarn electron ./dist/apps/electron/main.js",
    "************ELECTRON SETUP BUILD************************": "",
    "electron:setup:build": "del-cli release/* && yarn electron:build && yarn build:electron-prod && electron-builder -w",
    "electron:setup:build-dev": "yarn electron:build && electron-builder -w",
    "electron:setup:build-dev:dir": "yarn electron:build && electron-builder -w --dir",
    "electron:setup:build-beta": "del-cli release/* && yarn electron:build && yarn build:electron-dev && electron-builder -w",
    "electron:setup:publish": "yarn build:electron-prod && yarn electron:build && electron-builder -w -p always",
    "postinstall": "electron-builder install-app-deps",
    "release:full": "yarn build:prod && firebase deploy -P default --only hosting && yarn electron:setup:publish",
    "pack": "del-cli release/* && electron-builder --dir",
    "electron-builder": "electron-builder",
    "***********SSR STUFF***************": "",
    "build:ssr": "yarn gt:fetch && yarn webpack:ssr & node ssr/copy-translations.js & yarn copy:ssr & exit 0",
    "build:ssr:ci": "yarn build:functions && yarn build:functions && yarn gt:fetch && yarn webpack:ssr; node ssr/copy-translations.js; cp -r ./dist functions; exit 0",
    "webpack:ssr": "yarn nx run client:server && webpack --config webpack.server.config.js",
    "serve:ssr": "node dist/client-webpack/server.js",
    "start:ssr": "yarn build:ssr & yarn serve:ssr",
    "gt:fetch": "node ssr/gt-data-fetcher.js",
    "copy:ssr": "copyfiles ./dist/** ./functions",
    "*********** STATS STUFF **************": "",
    "build:stats": "yarn prebuild && yarn nx build --project=client --configuration production --stats-json",
    "bundle-analyzer": "webpack-bundle-analyzer dist/apps/client/stats.json",
    "*********** API STUFF **************": "",
    "api:build": "nx run api:build:production",
    "api:deploy": "yarn api:build && gcloud builds submit --config ./apps/api/cloudbuild.yaml . && gcloud beta run deploy api --image gcr.io/ffxivteamcraft/api --region us-central1",
    "api:start": "nx run api:serve",
    "lodestone-api:build": "nx run lodestone-api:build:production",
    "lodestone-api:deploy": "yarn lodestone-api:build && gcloud builds submit --config ./apps/lodestone-api/cloudbuild.yaml . && gcloud beta run deploy lodestone-api --image gcr.io/ffxivteamcraft/lodestone-api --region us-central1"
  },
  "private": true,
