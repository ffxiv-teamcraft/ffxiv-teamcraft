{
  "name": "ffxiv-teamcraft",
  "description": "Collaborative crafting made easy",
  "homepage": "https://ffxivteamcraft.com",
  "author": "FFXIV Teamcraft",
  "version": "7.6.21",
  "license": "MIT",
  "main": "desktop/main.js",
  "build": {
    "appId": "ffxiv-teamcraft",
    "forceCodeSigning": true,
    "productName": "FFXIV Teamcraft",
    "directories": {
      "output": "release"
    },
    "files": [
      "desktop/main.js",
      "desktop/oauth.js",
      "desktop/machina.js",
      "dist"
    ],
    "extraResources": [
      {
        "from": "./node_modules/node-machina-ffxiv/MachinaWrapper",
        "to": "MachinaWrapper",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./desktop",
        "to": "MachinaWrapper",
        "filter": [
          "**/*.exe"
        ]
      }
    ],
    "protocols": {
      "name": "ffxiv-teamcraft",
      "schemes": [
        "teamcraft"
      ]
    },
    "mac": {
      "category": "public.app-category.games"
    },
    "win": {
      "icon": "./tools/build/icon.ico",
      "publisherName": "FFXIV Teamcraft",
      "target": "squirrel",
      "sign": "./tools/build/sign-windows.js",
      "publish": {
        "provider": "github",
        "releaseType": "release"
      }
    },
    "squirrelWindows": {
      "iconUrl": "https://cdn.ffxivteamcraft.com/favicon.ico",
      "remoteReleases": "https://update.ffxivteamcraft.com/"
    }
  },
  "scripts": {
    "ng-high-memory": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "ng": "ng",
    "prebuild": "node ./tools/build/prebuild.js && npm run update-lazy-files",
    "assets:hash": "node ./tools/build/prod-assets-hash.js",
    "start-high-memory": "npm run prebuild && ng-high-memory serve --project=client",
    "start": "npm run prebuild && npm run ng-high-memory serve -- --project=client",
    "build:watch": "npm run prebuild && npm run ng-high-memory build -- --project=client --watch --configuration=electron-dev",
    "build:prod": "npm run prebuild && npm run ng-high-memory build -- --project=client --prod --no-progress && npm run assets:hash",
    "build:functions": "npm run prebuild && npm run ng-high-memory build -- --project=client --configuration=functions --no-progress",
    "build:electron-prod": "npm run prebuild && npm run ng-high-memory build -- --project=client --configuration=electron && npm run assets:hash",
    "build:electron-dev": "npm run prebuild && npm run ng-high-memory build -- --project=client --configuration=electron-dev",
    "build:beta": "npm run prebuild && npm run ng-high-memory build -- --project=client --configuration=beta --no-progress",
    "standard-version": "standard-version",
    "standard-version:patch": "git fetch origin && standard-version --release-as patch && npm run standard-version:after",
    "standard-version:minor": "git fetch origin && standard-version --release-as minor && npm run standard-version:after",
    "standard-version:dry": "standard-version --dry-run",
    "standard-version:after": "npm run prebuild && git commit -am \"chore(release): Patch notes update\"",
    "release:prod": "git push --follow-tags origin master",
    "release:beta": "npm run standard-version -- --prerelease beta && git push --follow-tags origin beta",
    "test": "npm run prebuild && ng test --project=client --watch=false --code-coverage",
    "codecov": "cat coverage/lcov.info | codecov",
    "lint": "ng lint && jsonlint-cli ./src/assets/i18n/*.json",
    "e2e": "npm run prebuild && ng e2e client-e2e",
    "e2e:watch": "npm run prebuild && ng e2e client-e2e --watch",
    "e2e:ci": "npm run prebuild && ng e2e client-e2e --headless",
    "commitmsg": "validate-commit-msg",
    "prerelease": "npm test",
    "commit": "git-cz",
    "electron": "electron",
    "electron:start": "npm run electron ./desktop/main.js",
    "electron:setup:build": "del-cli release/* && npm run build:electron-prod && electron-builder -w",
    "electron:setup:build-dev": "electron-builder -w",
    "electron:setup:build-beta": "del-cli release/* && npm run build:electron-dev && electron-builder -w",
    "electron:setup:publish": "npm run build:electron-prod && electron-builder -w -p always",
    "postinstall": "ngcc && electron-builder install-app-deps",
    "release:full": "npm run build:prod && firebase deploy -P default --only hosting && npm run electron:setup:publish",
    "pack": "electron-builder --dir",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "electron-builder": "electron-builder",
    "update-lazy-files": "node ./tools/build/lazy-files-updater.js",
    "***********SSR STUFF***************": "",
    "build:ssr": "npm run gt:fetch && npm run webpack:ssr & node ssr/copy-translations.js & npm run copy:ssr & exit 0",
    "build:ssr:ci": "npm run build:functions && npm run build:functions && npm run gt:fetch && npm run webpack:ssr; node ssr/copy-translations.js; cp -r ./dist functions; exit 0",
    "webpack:ssr": "npm run ng-high-memory run client:server && webpack --config webpack.server.config.js --no-progress",
    "serve:ssr": "node dist/client-webpack/server.js",
    "start:ssr": "npm run build:ssr & npm run serve:ssr",
    "gt:fetch": "node ssr/gt-data-fetcher.js",
    "copy:ssr": "copyfiles ./dist/** ./functions",
    "*********** STATS STUFF **************": "",
    "build:stats": "npm run prebuild && npm run ng-high-memory build -- --project=client --prod --stats-json",
    "bundle-analyzer": "webpack-bundle-analyzer dist/apps/client/stats.json"
  },
  "private": true,
  "dependencies": {
    "electron-config": "^2.0.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^3.0.6",
    "is-elevated": "^3.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^7.0.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.10",
    "node-machina-ffxiv": "^2.39.1",
    "request": "^2.88.2",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular-devkit/core": "^11.0.2",
    "@angular-devkit/schematics": "^11.0.2",
    "@angular/animations": "^11.0.1",
    "@angular/cdk": "^9.2.1",
    "@angular/cli": "^11.0.2",
    "@angular/common": "^11.0.1",
    "@angular/compiler": "^11.0.1",
    "@angular/compiler-cli": "^11.0.1",
    "@angular/core": "^11.0.1",
    "@angular/fire": "^6.1.4",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.0.1",
    "@angular/language-service": "^11.0.1",
    "@angular/platform-browser": "^11.0.1",
    "@angular/platform-browser-dynamic": "^11.0.1",
    "@angular/router": "^11.0.1",
    "@ffxiv-teamcraft/crafting-solver": "^1.3.2",
    "@ffxiv-teamcraft/simulator": "^2.9.1",
    "@ffxiv-teamcraft/simulator-cn": "npm:@ffxiv-teamcraft/simulator@2.7.1",
    "@ffxiv-teamcraft/simulator-kr": "npm:@ffxiv-teamcraft/simulator@2.7.1",
    "@firebase/auth-types": "^0.10.1",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-free-webfonts": "^1.0.9",
    "@kaiu/ng-serializer": "1.1.1",
    "@kaiu/serializer": "1.4.0",
    "@ngrx/effects": "^10.0.1",
    "@ngrx/entity": "^10.0.1",
    "@ngrx/router-store": "^10.0.1",
    "@ngrx/schematics": "^10.0.1",
    "@ngrx/store": "^10.0.1",
    "@ngrx/store-devtools": "^10.0.1",
    "@nguniversal/express-engine": "^11.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@nrwl/angular": "^10.4.4",
    "@nrwl/cypress": "^10.4.4",
    "@nrwl/express": "^10.4.4",
    "@nrwl/node": "^10.4.4",
    "@nrwl/workspace": "^10.4.4",
    "@swimlane/dragula": "^3.8.0",
    "@swimlane/ngx-charts": "^14.0.0",
    "@swimlane/ngx-dnd": "^8.1.0",
    "@types/electron": "^1.6.10",
    "@types/express-serve-static-core": "^4.17.13",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.123",
    "@types/node": "^12.11.1",
    "@types/semver": "^5.4.0",
    "@xivapi/angular-client": "1.7.1",
    "angular-feather": "^5.3.0",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.9.0",
    "apollo-cache-inmemory": "^1.6.0",
    "apollo-client": "^2.6.0",
    "apollo-link": "^1.2.11",
    "classlist.js": "^1.1.20150312",
    "codecov": "^3.6.5",
    "codelyzer": "^5.1.2",
    "colors": "^1.3.2",
    "commitizen": "^2.9.6",
    "copyfiles": "^2.1.0",
    "core-js": "^2.5.4",
    "cypress": "^4.4.0",
    "cz-conventional-changelog": "^2.1.0",
